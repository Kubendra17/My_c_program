include<stdio.h>
int main()
{
    int marks;
    printf("enter the marks");
    scanf("%d,&marks");
    if(marks>=85)
      {printf("grade A");
      
    }
    else if(marks>=70)
      {printf("grade B");
    }
    else if (marks>=50)
     {printf("grede  C");
    }
    else
     { printf("grade D");
    
    }
   return 0;

}
